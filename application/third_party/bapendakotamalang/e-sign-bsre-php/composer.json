{
    "name": "bapendakotamalang/e-sign-bsre-php",
    "description": "Package E-Sign BSrE untuk memudahkan penggunaan E-Sign BSrE (Balai Sertifikasi Elektronik) API dari BSSN (Badan Sandi dan Siber Negara)",
    "keywords": [
        "bapendakotamalang",
        "e-sign-bsre-php"
    ],
    "homepage": "https://github.com/bapendakotamalang/e-sign-bsre-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bapenda Kota Malang",
            "email": "bppdmalang@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BAPENDAKOTAMALANG\\ESignBsrePhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BAPENDAKOTAMALANG\\ESignBsrePhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}